#include <iostream>
#include <vector>
#include <limits>
using namespace std;

int main() {
    const int INF = 1e9;

    vector<vector<int>> g = {
        {0,6,7,3,0,0,0,0},
        {6,0,2,9,0,9,0,0},
        {7,2,0,0,0,7,0,4},
        {3,9,0,0,5,0,0,0},
        {0,0,0,5,0,4,1,0},
        {0,9,7,0,4,0,8,0},
        {0,0,0,0,1,8,0,5},
        {0,0,4,0,0,0,5,0}
    };

    int n = 8;
    vector<int> min_e(n, INF), sel_e(n, -1);
    vector<bool> used(n, false);

    min_e[0] = 0;
    int total = 0;

    for (int i = 0; i < n; i++) {
        int v = -1;

        for (int j = 0; j < n; j++)
            if (!used[j] && (v == -1 || min_e[j] < min_e[v]))
                v = j;

        used[v] = true;
        total += min_e[v];

        if (sel_e[v] != -1)
            cout << sel_e[v]+1 << " - " << v+1 << endl;

        for (int to = 0; to < n; to++)
            if (g[v][to] < min_e[to]) {
                min_e[to] = g[v][to];
                sel_e[to] = v;
            }
    }

    cout << "Total weight = " << total;
}
    }

    return 0;
}
